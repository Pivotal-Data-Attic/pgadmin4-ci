- name: placeholder-resource
  type: time
  source: {interval: 1m}

- name: <%= source_name + render_branch_name %>
  type: git
  source:
    uri: <%= git_uri %>
    branch: <%= render_branch_name %>

- name: pipeline-ci
  type: git
  source:
    uri: https://github.com/greenplum-db/pgadmin4-ci.git
    branch: master

- name: resource-deploy-web-app
  type: cf
  source:
    api: https://api.run.pivotal.io
    username: PWS_USERNAME
    password: PWS_PASSWORD
    organization: PWS_ORG
    space: PWS_SPACE
    skip_cert_check: false

- name: pivotal-tracker
  type: tracker
  source:
    token: TRACKER_TOKEN
    project_id: "1946163"
    tracker_url: https://www.pivotaltracker.com

